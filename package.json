{
  "name": "note",
  "version": "1.0.0",
  "private": true,
  "description": "A project on learning notes",
  "keywords": [
    "vuepress",
    "vuepress-theme-hope",
    "personal-blog"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:vite": "vuepress-vite build src",
    "build:vite-debug": "cross-env DEBUG=vuepress* vuepress-vite build src",
    "commit": "pnpm git-cz",
    "dev:vite": "vuepress-vite dev src",
    "dev:vite-clean": "vuepress-vite dev src --clean-cache",
    "dev:vite-debug": "cross-env DEBUG=vuepress* vuepress-vite dev src",
    "lint": "pnpm lint:prettier --write && pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint . --ext .{js,cjs,ts,vue}",
    "lint:mdlint": "markdownlint **/*.md",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint src/.vuepress/**/*.{css,scss}",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vue/repl": "^4.1.1",
    "@vuepress/bundler-vite": "2.0.0-rc.8",
    "@waline/client": "^3.1.3",
    "chart.js": "^4.4.2",
    "echarts": "^5.5.0",
    "flowchart.ts": "^3.0.0",
    "katex": "^0.16.9",
    "mathjax-full": "^3.2.2",
    "mermaid": "^10.8.0",
    "reveal.js": "^5.0.5",
    "vue": "^3.4.21",
    "vuepress": "2.0.0-rc.8",
    "vuepress-plugin-search-pro": "2.0.0-rc.27",
    "vuepress-theme-hope": "2.0.0-rc.27"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-git": "^1.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.11",
    "markdownlint-cli": "^0.39.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sort-package-json": "^2.8.0",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.4.2"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "auther": "white"
}
