{
  "name": "note",
  "version": "1.0.0",
  "private": true,
  "description": "A project on learning notes",
  "keywords": [
    "vuepress",
    "vuepress-theme-hope",
    "personal-blog"
  ],
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:vite": "vuepress-vite build src",
    "build:vite-debug": "cross-env DEBUG=vuepress* vuepress-vite build src",
    "commit": "pnpm git-cz",
    "dev:vite": "vuepress-vite dev src",
    "dev:vite-clean": "vuepress-vite dev src --clean-cache",
    "dev:vite-debug": "cross-env DEBUG=vuepress* vuepress-vite dev src",
    "lint": "pnpm lint:prettier --write && pnpm lint:eslint --fix && pnpm lint:stylelint --fix",
    "lint:check": "pnpm lint:prettier && pnpm lint:eslint && pnpm lint:stylelint",
    "lint:eslint": "eslint . --ext .{js,cjs,ts,vue}",
    "lint:mdlint": "markdownlint **/*.md",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint src/.vuepress/**/*.{css,scss}",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-git"
    }
  },
  "dependencies": {
    "@vue/repl": "^2.6.1",
    "@vuepress/cli": "2.0.0-beta.67",
    "@vuepress/client": "2.0.0-beta.67",
    "@waline/client": "^2.15.8",
    "chart.js": "^4.4.0",
    "echarts": "^5.4.3",
    "flowchart.ts": "^1.0.1",
    "katex": "^0.16.9",
    "mathjax-full": "^3.2.2",
    "mermaid": "^10.6.0",
    "reveal.js": "^4.6.1",
    "vue": "^3.3.6",
    "vuepress-plugin-search-pro": "2.0.0-beta.242",
    "vuepress-theme-hope": "2.0.0-beta.242",
    "vuepress-vite": "2.0.0-beta.67"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "commitizen": "^4.3.0",
    "cross-env": "7.0.3",
    "cz-git": "^1.7.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "markdownlint-cli": "^0.37.0",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "sort-package-json": "^2.6.0",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.9.2",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8",
    "pnpm": ">=8"
  },
  "auther": "white"
}
